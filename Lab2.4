<?php
$my_array = array('EHC', 'HackYourLimits', 'Training');
$maxlength=0;
$minlength= 1000;
foreach ($my_array as $array){
$length = strlen($array);
if ($length<$minlength){
    $minlength=$length;
}
if ($length>$maxlength){
    $maxlength=$length;
}
}
echo "minlength :$minlength; <br>";
echo "maxlength :$maxlength; ";
?>
